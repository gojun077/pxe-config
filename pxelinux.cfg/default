DEFAULT menu.c32
PROMPT 0
MENU TITLE PXE Boot Menu for Legacy BIOS

LABEL arch64_http
  TEXT HELP
  Boot the Arch Linux (x86_64) live medium (Using HTTP).
  It allows you to install Arch Linux or perform system maintenance.
  ENDTEXT
  MENU LABEL Boot Arch Linux (x86_64) (HTTP)
  LINUX images/arch_x64/vmlinuz-archlinux-x64
  INITRD images/arch_x64/intel_ucode.img,images/arch_x64/archiso.img
  APPEND archisobasedir=arch archiso_http_srv=http://192.168.95.97:8080/
  SYSAPPEND 3

LABEL fedora23
  MENU LABEL Boot f23 (HTTP)
  KERNEL images/fedora23_x64/vmlinuz
  INITRD images/fedora23_x64/initrd.img
  APPEND ip=dhcp inst.repo=http://192.168.95.97:8080 inst.vnc inst.vncconnect=192.168.95.97:5500 ksdevice=link inst.ks=http://192.168.95.97:8090/f23-3node.ks

LABEL xenial16.04_netinst
  MENU LABEL Boot Ubuntu 16.04 (netinst)
  KERNEL images/xenial_x64/linux
  INITRD  images/xenial_x64/initrd.gz
  APPEND ip=dhcp

LABEL xenial16.04_netinst-Netconsole
  MENU LABEL Boot Ubuntu 16.04 (netinst over Netconsole)
  KERNEL images/xenial_x64/linux
  INITRD  images/xenial_x64/initrd.gz
  APPEND ip=dhcp preseed/url=http://192.168.95.97:8000/netconsole.cfg

LABEL xenial16.04_dvd
  MENU LABEL Boot Ubuntu 16.04 DVD
  KERNEL images/xenialDVD/linux
  INITRD images/xenialDVD/initrd.gz
  APPEND ip=dhcp ks=http://192.168.95.97:8000/kickstart-ubuntu.cfg
