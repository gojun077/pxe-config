DEFAULT menu.c32
PROMPT 0
TIMEOUT 50
ONTIMEOUT RHEL7.0
MENU TITLE PXE Boot Menu for Legacy BIOS

LABEL arch64_http
  TEXT HELP
  Boot the Arch Linux (x86_64) live medium (Using HTTP).
  It allows you to install Arch Linux or perform system maintenance.
  ENDTEXT
  MENU LABEL Boot Arch Linux (x86_64) (HTTP)
  LINUX images/archlinux/vmlinuz
  INITRD images/archlinux/intel_ucode.img,images/arch_x64/archiso.img
  APPEND archisobasedir=arch archiso_http_srv=http://192.168.95.97:8080/
  SYSAPPEND 3

LABEL fedora23
  MENU LABEL Boot f23 (HTTP)
  KERNEL images/fedora23_x64/vmlinuz
  INITRD images/fedora23_x64/initrd.img
  APPEND ip=dhcp inst.repo=http://192.168.95.97:8080 inst.vnc inst.vncconnect=192.168.95.97:5500 ksdevice=link inst.ks=http://192.168.95.97:8000/f23-3node.ks

LABEL RHEL5.11
  MENU LABEL Boot RHEL5.11 (ks install no x11)
  KERNEL images/rhel5.11/vmlinuz
  INITRD images/rhel5.11/initrd.img
  APPEND ip=dhcp method=http://192.168.95.97:8080 vnc vncconnect=192.168.95.97:5500 ksdevice=link ks=http://192.168.95.97:8000/rhel5-auto-ks.cfg

LABEL RHEL5.11 X11
  MENU LABEL Boot RHEL5.11 X11 (ks install)
  KERNEL images/rhel5.11/vmlinuz
  INITRD images/rhel5.11/initrd.img
  APPEND ip=dhcp method=http://192.168.95.97:8080 vnc vncconnect=192.168.95.97:5500 ksdevice=link ks=http://192.168.95.97:8000/rhel5-ks-x11-SKT-Bundang.cfg

